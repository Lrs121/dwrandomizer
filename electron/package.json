{
  "name": "dwrandomizer",
  "version": "3.0.3",
  "description": "Dragon Warrior Randomizer",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder -wl",
    "dist-linux": "electron-builder -l",
    "dist-win": "electron-builder -w",
    "dist-mac": "electron-builder -m"
  },
  "build": {
    "appId": "com.dwrandomizer",
    "linux": {
      "category": "Gaming",
      "target": [
        "snap",
        "AppImage",
        "deb"
      ]
    },
    "mac": {
      "category": "Gaming",
      "target": [
        "dmg"
      ]
    },
    "win": {
      "target": [
        "portable"
      ]
    }
  },
  "repository": "https://github.com/mcgrew/dwrandomizer",
  "keywords": [
    "Dragon",
    "Warrior",
    "Randomizer"
  ],
  "author": {
    "name": "mcgrew",
    "email": "tjmcgrew@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "electron": "^19.0.7",
    "electron-builder": "^23.1.0"
  }
}
